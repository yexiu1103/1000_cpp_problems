Author: Silly Sushi
*/
#include<iostream>
using namespace std;

int main(){
    int n;
    cin >> n;
    long result = 1;

    if(n%2 != 0){
        for(int i=1; i<=n; i++){
            if(n%i == 0){
                result*=i;
            }
        }
    }
    cout<<result;
    return 0;
}
