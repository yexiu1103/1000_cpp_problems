#include <iostream>
using namespace std;

int main(){
    int n;
    float sum = 0;
    int i=1;

    do{
        cin >> n;
        if(n<1){
            cout << "Enter again. n>=1.\n";
        }
    }while(n<1);

    while(i<=n){
        sum = sum + 1.0/i;
        i++;
    }

    cout << sum;
    return 0;
}
