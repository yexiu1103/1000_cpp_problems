*/
#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    int count = 0;

    for(int i=1; i<=n; i++){
        if(n%i == 0){
            if(i%2 == 0){
                count+=1;
            }
        }
    }
    cout << count;
    return 0;
}
