#include <iostream>
using namespace std;

int main()
{
    int a;
    cin >> a;
    
   if(a>0 && a<=3){
       cout << "Thang thuoc Quy 1";
   }
   else if(a>=4 && a<=6){
       cout << "Thang thuoc Quy 2";
   }
   else if(a>=7 && a<=9){
       cout << "Thang thuoc Quy 3";
   }
   else
   cout << "Thang thuoc Quy 4";
    
    return 0;
}
