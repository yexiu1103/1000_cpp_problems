#include <iostream>
using namespace std;

int main() {
    int m, n;
    cout << "Nhap kich thuoc hinh chu nhat (m x n): ";
    cin >> m >> n;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cout << "*"; // In dấu sao
        }
        cout << endl; // Xuống dòng sau mỗi hàng
    }

    return 0;
}
