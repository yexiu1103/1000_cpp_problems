#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int n;
    cin>>n;
    
    double arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    
    bool soLonHon=false;
    for(int i=0; i<n; i++){
        if(arr[i]>2003){
            cout<<arr[i];
            soLonHon=true;
            break;
        }
    }
    
    if(!soLonHon)
    cout << 0;
    
    return 0;
}
