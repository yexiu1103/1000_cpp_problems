#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int n;
    cin>>n;
    
    double arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    
    // TÃ¬m min, max
    double max = arr[0];
    double min = arr[0];
    for(int i=0; i<n; i++){
        if(max<arr[i]){
            max=arr[i];
        }
        if(min>arr[i]){
            min=arr[i];
        }
    }
    
    int a,b;
    a=floor(min);
    b=ceil(max);
    cout << "[" << a << ";" << b << "]";
    
    return 0;
}
