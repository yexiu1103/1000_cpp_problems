#include <iostream>
using namespace std;

int main() {
    int x;
    cin >> x;
    
    int n;
    cout << "Nhap so luong phan tu trong mang: ";
    cin >> n;

    int arr[n];
    cout << "Nhap cac phan tu trong mang: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    int count = 0; 
    for (int i = 0; i < n; i++) {
        if (arr[i] == x) {
            count++;
        }
    }

    cout << "Tan suat xuat hien trong mang: " << count << endl;
    return 0;
}
