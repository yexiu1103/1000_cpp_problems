#include <iostream>
using namespace std;

int main(){
    int n;
    cin >> n;
    
    int arr[n];
    for(int i=0; i<n; i++){
        cin >> arr[i];
    }
    
    bool positive = false;
    for(int i=n-1; i>0; i--){
        if(arr[i]%2 == 0){
            cout << arr[i];
            positive = true;
            break;
        }
    }
    
    if (!positive){
        cout << "-1";
    }
    return 0;
}
