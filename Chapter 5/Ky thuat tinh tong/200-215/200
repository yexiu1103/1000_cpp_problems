#include <iostream>
#include <cmath>
using namespace std;


int main() {
    int n;
    cout << "Nhap so luong phan tu trong mang: ";
    cin >> n;

    int arr[n];
    cout << "Nhap cac phan tu trong mang: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    int sum=0;
    for(int i=0; i<n; i++){
        sum+=arr[i];
    }

    cout << sum; 
    return 0;
}

