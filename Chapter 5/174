
#include <iostream>
using namespace std;
int main()
{
    int n;
    cin >> n;
    if(n<2)
    cout<< "Mang phai co nhieu hon 2 gia tri";
    
    double arr[n];
    for(int i=0; i<n; i++){
        cin >> arr[i];
    }
    
    for(int i=0; i<n; i++){
        for(int j=i; j<n; j++){
            if(arr[i]<=arr[j]){
                cout << "(" << arr[i] << ", " << arr[j] << ")" << endl;
            }
        }
    }
    return 0;
}
