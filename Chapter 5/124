#include <iostream>
using namespace std;

bool kiemTra(int arr[], int n){
    for(int i=0; i<n; i++){
        if(arr[n] %2 == 0 && arr[n] <2004){
            return true;
        }
        else 
    return false;
    }
    
}

int main(){
    int n;
    cin >> n;

    int arr[n];
    for(int i=0; i<n; i++){
        cin >> arr[i];
    }
    
    if(kiemTra(arr, n)){
        cout << "Yes";
    }
    else
    cout << "No";
    
    return 0;
}
