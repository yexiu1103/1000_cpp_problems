
#include <iostream>
using namespace std;
int main()
{
   int n;
   cin >> n;
   
   double arr[n];
   for(int i=0; i<n; i++){
       cin >> arr[i];
   }
   
   double min=arr[0];
   for(int i=0;i<n; i++){
       if(min > arr[i]){
           min = arr[i];
       }
   }

cout << min;
    return 0;
}
